                    <h4 class="card-title">주문 상태</h4>
                    <div class="table-responsive">
                      <table class="table text-center">
                        <thead>
                          <tr>
                            <th>
                              <div class="form-check form-check-muted m-0">
                                <label class="form-check-label">
                                  <input type="checkbox" class="form-check-input">
                                </label>
                              </div>
                            </th>
                            <th> 아이디 </th>
                            <th> 주문번호 </th>
                            <th> 가격 </th>
                            <th> 상품명 </th>
                            <th> 지불방식 </th>
                            <th> 주문일 </th>
                            <th> 수량 </th>
                          </tr>
                        </thead>
                        <tbody>
                        <c:choose>
                        	<c:when test="${fn:length(listp) eq 0}">
                        		<tr>
                        			<td colspan="8">
                        				There is no date!
                        			</td>
                        		</tr>
							</c:when>
							<c:otherwise>
								<c:forEach items="${listp}" var="listp" varStatus="statuslistp">
		                          <tr>
		                          	<td>
		                              <div class="form-check form-check-muted m-0">
		                                <label class="form-check-label">
		                                  <input type="checkbox" class="form-check-input">
		                                </label>
		                              </div>
		                            </td>
		                            <td> <c:out value="${listp.id }"/> </td>
		                            <td> <c:out value="${listp.purchaseSeq }"/> </td>
		                            <td> <fmt:formatNumber value="${listp.price }" pattern="#,###"/>원 </td>
		                            <td> <c:out value="${listp.product_name }"/> </td>
		                            <td>
		                            	<c:choose>
		                            		<c:when test="${listp.paytype eq 1 }"><div class="badge badge-outline-success">카드결제</div></c:when>
		                            		<c:otherwise><div class="badge badge-outline-warning">계좌이체</div></c:otherwise>
		                            	</c:choose>
		                            </td>
		                            <td> <fmt:formatDate value="${listp.regtime }" pattern="yyyy-mm-dd HH:mm:ss" /> </td>
		                            <td> <c:out value="${listp.quantity }"/> </td>
		                          </tr>
                          		</c:forEach>
                          	</c:otherwise>
                         </c:choose>
                        </tbody>
                      </table>
                    </div>
